.headTab {
  background-color: #fff;
  height: 100rpx;
  border-bottom: 1px solid #e9e9e9;
  text-align: center;
}
.headTabFake {
  background-color: #fff;
  height: 100rpx;
  width: 100%;
}
.headTabFiex {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 99;
}
.headTab .tabTxt {
  font-size: 35rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  display: inline-block;
  margin: 0 50rpx;
  padding: 0 6rpx;
}
.headTab .tabTxt.active {
  color: #ed7b97;
  box-sizing: border-box;
  border-bottom: 5rpx solid #ed7b97;
}
.header {
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
}
.header .inputCon {
  display: inline-flex;
  height: 75rpx;
  width: 700rpx;
  margin: 0 auto;
  flex-direction: row;
  align-items: center;
  background-color: #f4f4f4;
  border-radius: 35rpx;
  overflow: hidden;
}
.header .inputImg {
  width: 50rpx;
  height: 50rpx;
  margin-left: 20rpx;
}
.header .inputSty {
  border-radius: 25rpx;
  padding: 0 15rpx;
}
.header .plcolor {
  color: #bababb;
}
.channelList {
  width: 750rpx;
  display: flex;
  margin: 20rpx 0;
}
.channelList .channelType {
  text-align: center;
  width: 150rpx;
}
.channelList .channelType .typeImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.channelList .channelType .typeTit {
  font-size: 24rpx;
}
.trans {
  width: 750rpx;
  height: 20rpx;
  background-color: #f4f4f4;
}
.channelRecord {
  margin: 20rpx 0;
}
.scrollRecord {
  height: 315rpx;
}
.channelRecord .recordTit {
  font-size: 26rpx;
  color: #454545;
  margin-left: 20rpx;
  letter-spacing: 1rpx;
}
.channelRecord .recordList {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: nowrap;
  width: 1150rpx;
}
.channelRecord .recordList .recordBox {
  width: 210rpx;
  height: 280rpx;
  position: relative;
  text-align: center;
  margin-right: 20rpx;
  box-shadow: 0 0 2rpx #9f9f9f;
  border-radius: 15rpx;
}
.channelRecord .recordList .recordBox:first-child {
  margin-left: 20rpx;
}
.channelRecord .recordList .recordBox .bgImg {
  width: 210rpx;
  height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.channelRecord .recordList .recordBox .cenImg {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  border: 2px solid #ffffff;
  margin-top: 50rpx;
}
.channelRecord .recordList .recordBox .channelName {
  font-size: 26rpx;
  font-weight: bold;
}
.channelRecord .recordList .recordBox .recordTime {
  font-size: 24rpx;
  color: #9f9f9f;
  width: 140rpx;
  margin: 0 auto;
}
.hotChannel {
  margin: 20rpx 0 40rpx;
}
.hotChannel .hotTit {
  font-size: 26rpx;
  color: #454545;
  margin-left: 20rpx;
  letter-spacing: 1rpx;
}
.hotChannel .hotRightIcon {
  float: right;
  margin-right: 20rpx;
}
.hotIcon {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.subChannel {
  margin: 20rpx 0;
}
.subChannel .hotTit {
  font-size: 26rpx;
  color: #454545;
  margin-left: 20rpx;
  letter-spacing: 1rpx;
}
.subChannel .hotRightIcon {
  float: right;
  margin-right: 60rpx;
}
.subChannel .hotTit .checkAll {
  float: right;
  margin-right: 35rpx;
}
.subChannel .hotTit .checkAll::after {
  content: "";
  width: 16rpx;
  height: 16rpx;
  border-top: 1rpx solid #adadad;
  border-right: 1rpx solid #adadad;
  border-bottom: 1rpx solid transparent;
  border-left: 1rpx solid transparent;
  position: absolute;
  margin-top: 9rpx;
  transform: rotate(45deg);
}
.subList .subBox {
  margin: 20rpx 20rpx;
}
.subList .subBox .boxTit {
  height: 90rpx;
}
.subList .subBox .boxTit .boxName {
  font-size: 30rpx;
  font-weight: bold;
}
.subList .subBox .boxTit .boxTime {
  font-size: 20rpx;
  color: #9f9f9f;
  margin-top: 10rpx;
}
.subList .subBox .boxTit .leftTit {
  float: left;
}
.subList .subBox .boxTit .goChannel {
  color: #dbb1c0;
  font-size: 26rpx;
  width: 220rpx;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  border: 1px solid #dbb1c0;
  float: right;
  border-radius: 10rpx;
  margin-top: 10rpx;
}
.subList .subBox .channelVideo {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.videoBox {
  width: 345rpx;
  border-radius: 15rpx 15rpx 0 0;
  overflow: hidden;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.videoBox .videoImgBox {
  width: 345rpx;
  height: 210rpx;
  position: relative;
}
.videoBox .videoImgBox .imgBg {
  width: 345rpx;
  height: 210rpx;
}
.videoBox .videoTitle {
  font-size: 26rpx;
  text-align: justify;
  margin: 10rpx 0 0;
  height: 70rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.bottomText {
  font-size: 26rpx;
  color: #9f9f9f;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #ebebeb;
}
.subChannel .subList .subBox:last-child .bottomText {
  border-bottom: none;
}

.channelArea {
  width: 750rpx;
  margin: 20rpx 0;
}
.channelArea .channelType {
  text-align: center;
  width: 25%;
  height: 160rpx;
  float: left;
}
.channelArea .channelType .typeImg {
  width: 90rpx;
  height: 90rpx;
}
.channelArea .channelType .typeTit {
  font-size: 24rpx;
}
.scvhe1, .scvhe2 {
  width: 100%;
}