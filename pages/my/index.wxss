  /* margin: 40rpx 0 0; */
}
.creation2 {
  margin-top: 40rpx;
}
.creation .top {
  width: 700rpx;
  margin: 0 auto;
}
.creation .top .tit {
  font-size: 28rpx;
  font-weight: bold;
