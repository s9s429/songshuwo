
.header {
  width: 100%;
  margin: 20rpx 0 0;
  overflow: hidden;
}
.msg .head {
  margin-left: 20rpx;
  float: left;
}
.msg .head .headimg {
  width: 150rpx;
  height: 150rpx;
}
.msg .mes {
  float: left;
  margin-left: 20rpx;
  margin-top: 30rpx;
}
.mes .name {
  font-size: 28rpx;
  color: #f17796;
  font-weight: bold;
  letter-spacing: 1rpx;
}
.sexicon {
  width: 20rpx;
  height: 23rpx;
  margin-left: 10rpx;
}
.lvicon {
  width: 45rpx;
  height: 20rpx;
  margin-left: 10rpx;
}
.yearvip {
  background-color: #f17796;
  color: #fff;
  font-size: 20rpx;
  border-right: 15rpx;
  width: 130rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  letter-spacing: 1rpx;
  border-radius: 8rpx;
  margin: 10rpx 0;
}
.icon {
  color: #b3b3b3;
  font-size: 24rpx;
}
.zone {
  float: right;
  color: #b3b3b3;
  font-size: 26rpx;
  margin-top: 67rpx;
  margin-right: 45rpx;
}
.zone::after {
  content: "";
  position: absolute;
  width: 15rpx;
  height: 15rpx;
  border-top: 1px solid #b3b3b3;
  border-right: 1px solid #b3b3b3;
  border-bottom: 1px solid transparent;
  border-left: 1px solid transparent;
  transform: rotate(45deg);
  margin-top: 10rpx;
}
.subList {
  width: 100%;
  margin-top: 30rpx;
}
.subList .item {
  float: left;
  width: 249rpx;
  text-align: center;
  /* border-right: 1px solid #bebebe;
  box-sizing: border-box; */
}
/* .subList .item:last-child {
  border-right: none;
} */
.line {
  float: left;
  width: 1.5rpx;
  height: 35rpx;
  background-color: #e3e3e3;
  margin-top: 20rpx;
}
.subList .item .zhu {
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.subList .item .fu {
  color: #b3b3b3;
  font-size: 24rpx;
}
.clear {
  clear: both;
}
.pinkArea {
  width: 700rpx;
  height: 110rpx;
  margin: 20rpx auto 0;
  background-color: #f17796;
  border-radius: 15rpx 15rpx 0 0;
  box-shadow: 0px -1px 20px rgba(241,119,150,.5);
}
.pinkArea .leftet {
  float: left;
  margin-left: 40rpx;
  margin-top: 20rpx;
}
.pinkArea .leftet .fir {
  color: #ffffff;
  font-size: 28rpx;
  letter-spacing: 2rpx;
}
.pinkArea .leftet .twi {
  color: #fff;
  font-size: 22rpx;
  letter-spacing: 2rpx;
  margin-top: 10rpx;
}
.pinkArea .righttet .goArea {
  width: 60rpx;
  height: 110rpx;
  float: right;
}
.pinkArea .righttet .goArea::after {
  content: "";
  position: absolute;
  width: 15rpx;
  height: 15rpx;
  border-top: 1px solid #fff;
  border-right: 1px solid #fff;
  border-bottom: 1px solid transparent;
  border-left: 1px solid transparent;
  transform: rotate(45deg);
  margin-top: 45rpx;
}
.body {
  width: 100%;
}
.fourbtn {
  margin-top: 30rpx;
}
.fourbtn .item {
