}
.list .item .tet {
  font-size: 26rpx;
}
.moreSer {
  width: 750rpx;
  /* margin: 0; */
}
.moreSer .top {
  width: 700rpx;
  margin: 0 auto;
}
.moreSer .top .tit {
  font-size: 28rpx;
  font-weight: bold;
} 
.moreSer .listt .itemm {
  margin-left: 20rpx;
  clear: both;
}
.moreSer .listt .itemm .left {
  font-size: 26rpx;
  float: left;
  margin-top: 30rpx;
  margin-bottom: 0rpx;
}
.moreSer .listt .itemm:last-child .left {
